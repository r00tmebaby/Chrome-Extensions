{
  "manifest_version": 3,
  "name": "Audio Normalizer & EQ",
  "version": "1.1.1",
  "description": "Normalize, boost, and equalize audio on any site, with per-site controls.",
  "permissions": ["activeTab", "scripting", "storage"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  }
}
